<div class="col-2 text-center">
  <%= render "/shared/user-sidebar" %>
</div>
<div class="col-8 bg-white">
  <div class="private-profile" id="private-profile">
    <%= render "shared/user-profile", type: "public_profile", current_view: "photo", photos_count: @photos_count, albums_count: @albums_count, followings_count: @followings_count, followers_count: @followers_count, is_following: @is_following, user: @user %>
    <div class="add-photo-div">
      <%= link_to 'Add Photos', new_photo_path, class: "btn btn-success float-end" %>
    </div>
    <div class="row">
      <%= @photos.each do |p| %>
        <%= render "photo", photo: p, type: "public_profile" %>
      <% end %>
    </div>
  </div>
</div>
<div class="col-2"></div>

<%= render "shared/modal", type: "photo" %>
<%= javascript_pack_tag 'photo-modal.js' %>
<%= javascript_pack_tag 'follow-feature.js' %>